<%- include("./partials/html-head") %>
<%- include("./partials/nav") %>

<main>
  <h1><%= title %></h1>
  <table>
    <thread>
      <tr>
        <th>Airline</th>
        <th>Airport</th>
        <th>Flight No</th>
        <th>Departure Date & Time</th>
      </tr>
    </thread>
    <tbody>
      <% flights.forEach(flight => { %>
        <tr>
          <td><%= flight.airline %> </td>
          <td><%= flight.airport %> </td>
          <td><%= flight. %> </td>
        </tr>
        




      <% }) %> 
    </tbody>
  </table>
</main>

<%- include("./partials/footer") %>
